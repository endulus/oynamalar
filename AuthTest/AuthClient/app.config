<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <customBinding>
                <binding name="httpx">
                    <security authenticationMode="UserNameOverTransport" allowInsecureTransport="true" />
                    <textMessageEncoding />
                    <httpTransport />
                </binding>
            </customBinding>
            <wsHttpBinding>
                <binding name="CustomBinding_ITestService">
                    <security mode="TransportWithMessageCredential">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" establishSecurityContext="false" />
                    </security>
                </binding>
                <binding name="WSHttpBinding_ITokenService" />
            </wsHttpBinding>
        </bindings> 
        <client>
            <endpoint address="http://localhost.:34886/TestService.svc" binding="customBinding"
                bindingConfiguration="httpx" contract="TestService.ITestService"
                name="CustomBinding_ITestService">
                <identity>
                    <userPrincipalName value="mct77\muhammed" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost.:34886/TokenService.svc" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_ITokenService" contract="TokenService.ITokenService"
                name="WSHttpBinding_ITokenService">
                <identity>
                    <userPrincipalName value="mct77\muhammed" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>